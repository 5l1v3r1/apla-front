    "scripts": {
        "extract-locales": "node util/extractLocales.js",
        "build-css": "node-sass-chokidar --include-path ./src/styles/scss --include-path ./node_modules src/styles/scss/sass.scss -o src/styles/built",
        "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/styles/scss --include-path ./node_modules src/styles/scss/sass.scss -o src/styles/built --watch --recursive",
        "start-js": "react-scripts-ts start",
        "start": "npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts-ts build",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject"
    },
